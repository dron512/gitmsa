#이미지 삭제
kubectl delete deploy catalog-service-deployment
docker rmi parkmyounghoi/catalog-service:msa.k8.0.1
docker rmi parkmyounghoi/catalog-service:msa.k8.0.2

#이미지 생성
docker build -t parkmyounghoi/catalog-service:msa.k8.0.3 .

#이미지 dockerhub 업로드 명령어
docker push parkmyounghoi/catalog-service:msa.k8.0.3